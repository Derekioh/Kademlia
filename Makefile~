RoutingTable.o: KBucket.h constants.h RoutingTable.h Node.h RoutingTable.cc
	g++ -c RoutingTable.cc
KBucket.o: KBucket.h KBucket.cc constants.h
	g++ -c KBucket.cc
Node.o: Node.h Node.cpp
	g++ -c Node.cpp

RoutingTableTest: RoutingTable.o KBucket.o Node.o
	g++ RoutingTable.o KBucket.o Node.o -o RoutingTableTest
clean:
	rm RoutingTableTest
	rm *.o
	rm *~
